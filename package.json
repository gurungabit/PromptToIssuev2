{
  "name": "prompt-to-issue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"DynamoDB,NextJS\" --prefix-colors \"cyan,magenta\" \"npm run db:start\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:start": "npm run db:up && npm run db:setup",
    "db:up": "docker compose -f docker-compose.dynamodb.yml up -d",
    "db:down": "docker compose -f docker-compose.dynamodb.yml down",
    "db:setup": "node scripts/setup-dynamodb-tables.js",
    "upackage": "npx npm-check-updates -u && npm install",
    "mcp:sync": "cd mcp/github-mcp && uv sync && cd ../gitlab-mcp && uv sync"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@aws-sdk/client-dynamodb": "^3.859.0",
    "@aws-sdk/lib-dynamodb": "^3.859.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.536.0",
    "nanoid": "^5.1.5",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "openai": "^5.11.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.9.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "undici": "^7.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "dynamodb-local": "^0.0.35",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
